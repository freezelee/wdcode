<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite3.2.dtd">
<urlrewrite>
    <rule>
        <from>goods_list/([0-9]+).html</from>
        <to>shop/goods_to_list.htm?entity.sortId=$1</to>
    </rule>
    <rule>
        <from>goods_list/([0-9]+)_([0-9]+).html</from>
        <to>shop/goods_list.htm?entity.sortId=$1&amp;entity.brandId=$2</to>
    </rule>
    <rule>
        <from>goods_list/([0-9]+)_([0-9]+)_*.html</from>
        <to>shop/goods_list.htm?entity.sortId=$1&amp;entity.brandId=$2&amp;entity.attributes.value=$3</to>
    </rule>
    <rule>
        <from>goods/([0-9]+).html</from>
        <to>shop/goods_theme_content.htm?entity.id=$1</to>
    </rule>
    <rule>
        <from>article/([0-9]+).html</from>
        <to>shop/article_theme_content.htm?entity.id=$1</to>
    </rule>
    <rule>
        <from>article_list/([0-9]+).html</from>
        <to>shop/category_theme_list.htm?entity.id=$1</to>
    </rule>
</urlrewrite>